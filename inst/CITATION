year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{Rramas}: Matrix Population Models",
         author = c(person("Marcelino", "de la Cruz Rot")),
         year = year,
         note = note,
         url = "https://CRAN.R-project.org/package=Rramas")